@model CodeLouSpotify.Models.Tracks
@{
    ViewData["Title"] = "SearchResult";
}

<h1>SearchResult</h1>
<div>
    @{
        for (var i = 0; i < Model.items.Length; i++)
        {
            
            <h2>@Model.items[i].name</h2>

            <img src="@Model.items[i].album.images[i].url" height="@Model.items[i].album.images[i].height" width="@Model.items[i].album.images[i].width" />

        }
    }

</div>

<p>Back to Search</p>

